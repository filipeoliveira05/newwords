{
  "cli": {
    "version": ">= 16.13.3",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "env": {
        "APP_VARIANT": "development",
        "ANDROID_PACKAGE": "com.filipeoliveira05.newwords.dev"
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "developmentClient": true,
      "android": {
        "buildType": "apk"
      }
    },
    "preview-isolated": {
      "extends": "preview",
      "channel": "isolated-test",
      "developmentClient": false
    },
    "production": {
      "autoIncrement": true,
      "channel": "production"
    }
  },
  "submit": {
    "production": {}
  }
}
